{
  "package": {
    "web": {
      "package": {
        "push": {
          "comment": "@teqfw/web-push entities and packages.",
          "entity": {
            "subscript": {
              "comment": "Users subscriptions.",
              "attr": {
                "id": {"type": "id"},
                "user_ref": {"type": "ref"},
                "date_created": {
                  "comment": "Date-time for subscription registration.",
                  "type": "datetime",
                  "default": "current"
                },
                "endpoint": {"type": "string"},
                "key_auth": {"type": "string"},
                "key_p256dh": {"type": "string"}
              },
              "index": {
                "uq_auth": {"type": "unique", "attrs": ["user_ref", "key_auth"]}
              },
              "relation": {
                "user": {
                  "attrs": ["user_ref"],
                  "ref": {"path": "/user", "attrs": ["id"]},
                  "action": {"delete": "cascade", "update": "cascade"}
                }
              }
            }
          }
        }
      }
    }
  },
  "refs": {
    "/user": ["id"]
  }
}
